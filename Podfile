target 'RealmCA' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RealmCA
  pod 'RealmSwift'

  target 'RealmCATests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RealmCAUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
